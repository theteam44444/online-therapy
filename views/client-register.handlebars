
<form class="signup-form form">
  <div>
    <label for="email-login">Email</label>
    <input type="text" id="email-signup" />
  </div>
  <div>
    <label for="password-signup">Password</label>
    <input type="password" id="password-signup" />
  </div>
  <div>
    <label for="firstname-signup">First Name</label>
    <input type="text" id="firstname-signup" />
  </div>
  <div>
    <label for="lastname-signup">Last Name</label>
    <input type="text" id="lastname-signup" />
  </div>
  <div>
    <label for="address-signup">Address</label>
    <input type="text" id="address-signup" />
  </div>
  <div>
    <label for="birthdate-signup">Birth Date</label>
    <input type="text" id="birthdate-signup" />
  </div>
